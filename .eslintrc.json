{
    "extends": "airbnb",
    "env": {
      "browser": true,
      "jest": true
    },
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ],
    "rules": {
      "linebreak-style":["windows"],
      "arrow-parens": ["error", "always"],
      "react/forbid-prop-types": 0,
      "react/jsx-filename-extension": 0
    }
  }
  